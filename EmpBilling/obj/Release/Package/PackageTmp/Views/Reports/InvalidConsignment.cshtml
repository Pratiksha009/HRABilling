@model IEnumerable<EmpBilling.EntityFr.Transaction>

@{
    ViewBag.Title = "Destinations";
    Layout = "~/Views/Shared/EmpLayout.cshtml";
}


<section class="content">

    <div class="row">
        <div class="col-xs-12">
            <div class="box box-info">
                <div class="box-header with-border">
                    <h4> Invalid Consignment</h4>
                </div><!-- /.box-header -->
                <div class="box-body">
                    <table id="table_display_block" class="table table-striped table-bordered table-responsive height-450">
                        <thead>
                            <tr>
                                <th>
                                    Sr No
                                </th>
                                <th>
                                    Consignment no
                                </th>
                                <th>
                                    Customer Id
                                </th>
                                <th>
                                    Booking Date
                                </th>
                                <th>
                                    Weight
                                </th>

                                <th>
                                    Pincode
                                </th>
                                <th>
                                    Mode
                                </th>
                                <th>
                                    Amount
                                </th>


                            </tr>
                        </thead>
                        <tbody>
                            @{ int i = 1;
                                foreach (var item in Model)
                                {
                            <tr>

                                <td>
                                    @i
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.Consignment_no)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.Customer_Id)
                                </td>
                                <td>
                                    @item.booking_date.Value.ToString("dd/MM/yyyy")
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.chargable_weight)
                                </td>

                                <td>
                                    @Html.DisplayFor(modelItem => item.Pincode)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.Mode)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.Amount)
                                </td>



                            </tr>

                                    i++;
                                }
                            }
                        </tbody>
                    </table>

                </div>
            </div>
        </div>
    </div>
</section>